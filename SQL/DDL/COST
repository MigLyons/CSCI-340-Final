CREATE TABLE mg167598.COST (
	costId INTEGER CONSTRAINT PK_COST PRIMARY KEY,
	vendor INTEGER CONSTRAINT FK_VENDOR REFERENCES VENDOR(vendorId),
	total INTEGER NOT NULL,
	paid BIT NOT NULL,
	);

